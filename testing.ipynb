    "and_strings = [\" & \", \" and \", \" og \"]\n",
    "\n",
    "def get_names_in_string(name):\n",
    "    for and_string in and_strings:\n",
    "        if and_string in name:\n",
    "            return name.split(and_string)\n",
    "    \n",
    "    return name.split(\" \")[0]\n",
    "    \n",
    "\n",
    "print(get_names_in_string(\"John\"))"
   ]
